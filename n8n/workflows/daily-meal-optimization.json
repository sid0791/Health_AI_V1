        "nodeCredentialType": "httpBearerAuth",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ]
        },
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "userIds",
              "value": "={{$json.processedUsers}}"
            },
            {
              "name": "notificationType",
              "value": "daily_meal_ready"
            },
            {
              "name": "template",
              "value": "daily_meal_optimization"
            },
            {
              "name": "priority",
              "value": "normal"
            }
          ]
        },
        "options": {}
