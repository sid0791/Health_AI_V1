            {
              "name": "workflowName",
              "value": "daily-meal-optimization"
            },
            {
              "name": "processedUsers",
              "value": "={{$json.processedUsers.length}}"
            },
            {
              "name": "executionTime",
              "value": "={{$now - $node['daily-schedule-trigger'].json.timestamp}}"
