{
  "name": "Daily Meal Plan Generation & Optimization",
  "nodes": [
    {
      "parameters": {},
      "id": "daily-schedule-trigger",
      "name": "Daily Schedule",
      "type": "n8n-nodes-base.scheduleTrigger",
      "typeVersion": 1.1,
      "position": [240, 300],
      "parameters": {
        "rule": {
          "interval": [
            {
              "field": "cronExpression",
              "expression": "0 7 * * *"
            }
          ]
        }
      }
    },
    {
      "parameters": {
        "url": "={{$env.API_BASE_URL}}/users/active-meal-plan-users",
        "authentication": "predefinedCredentialType",
        "nodeCredentialType": "httpBearerAuth",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Content-Type",
              "value": "application/json"
            },
            {
              "name": "X-Automation",
              "value": "daily-meal-optimization"
            }
          ]
        },
        "options": {}
      },
      "id": "get-active-users",
      "name": "Get Active Users",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [460, 300]
    },
    {
      "parameters": {
        "batchSize": 10,
        "options": {}
      },
      "id": "split-users-batch",
      "name": "Split Users into Batches",
      "type": "n8n-nodes-base.itemLists",
      "typeVersion": 3,
      "position": [680, 300]
    },
    {
      "parameters": {
        "url": "={{$env.API_BASE_URL}}/meal-planning/daily-optimization",
        "authentication": "predefinedCredentialType",
        "nodeCredentialType": "httpBearerAuth",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Content-Type",
              "value": "application/json"
            },
            {
              "name": "X-Batch-Processing",
              "value": "true"
            }
          ]
        },
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "userIds",
              "value": "={{$json.map(user => user.id)}}"
            },
            {
              "name": "optimizationType",
              "value": "daily_refresh"
            },
            {
              "name": "considerRecentFeedback",
              "value": true
            },
            {
              "name": "updateShoppingList",
              "value": true
            },
            {
              "name": "processedAt",
              "value": "={{$now}}"
            }
          ]
        },
        "options": {}
      },
      "id": "optimize-meal-plans",
      "name": "Optimize Meal Plans",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [900, 300]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "id": "optimization-success",
              "leftValue": "={{$json.success}}",
              "rightValue": true,
              "operator": {
                "type": "boolean",
                "operation": "equal"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "check-optimization-success",
      "name": "Check Optimization Success",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [1120, 300]
    },
    {
      "parameters": {
        "url": "={{$env.API_BASE_URL}}/meal-planning/grocery-list/update",
        "authentication": "predefinedCredentialType",
        "nodeCredentialType": "httpBearerAuth",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ]
        },
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "userIds",
              "value": "={{$json.processedUsers}}"
            },
            {
              "name": "updateType",
              "value": "daily_meal_optimization"
            },
            {
              "name": "includeNutritionalGoals",
              "value": true
            }
          ]
        },
        "options": {}
      },
      "id": "update-grocery-lists",
      "name": "Update Grocery Lists",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [1340, 200]
    },
    {
      "parameters": {
        "url": "={{$env.API_BASE_URL}}/nutrition/daily-analysis/batch",
        "authentication": "predefinedCredentialType",
        "nodeCredentialType": "httpBearerAuth",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Content-Type",
              "value": "application/json"
            }
